    <div class="container">
          <!-- Navbar -->

<!-- Navbar -->
<br><br>
<div class="mx-auto" style="width: 200px;">
  <h1>Admin list</h1>
</div>
<br><br>
      <table class="table align-middle mb-0 bg-white">
        <thead class="bg-light">
          <tr>
            <th>Email</th>
            <th>Username</th>
            <th>Role</th>
            <th>Verified</th>
            <th>Created Date</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% data.forEach(el=>{ %> 
          <tr>
            <td>
              <div class="d-flex align-items-center">
                <img
                    src="https://www.pngmart.com/files/21/Admin-Profile-PNG-Clipart.png"
                    alt=""
                    style="width: 45px; height: 45px"
                    class="rounded-circle"
                    />
                <div class="ms-3">
                  <p class="fw-bold mb-1"><%= el.Profile.firstName %>  <%= el.Profile.lastName %></p>
                  <p class="text-muted mb-0"><%= el.email %></p>
                </div>
              </div>
            </td>
            <td>
              <p class="fw-normal mb-1">@<%= el.username %></p>
            </td>
            <td>
              <p class="fw-normal mb-1"><%= el.role %></p>
            </td>

            <td>
              <button type="button" class="btn btn-link btn-sm btn-rounded">
                <a href="">isVerified</a>
              </button>
            </td>
            <td>
              <p class="fw-normal mb-1"><%= el.createdAt.toISOString().slice(0,10) %></p>
            </td>
            <td>
              <button type="button" class="btn btn-link btn-sm btn-rounded">
                <a href="/admin/admin-list/<%= el.id %>/delete">Delete</a>
              </button>
            </td>
          </tr>
          <% }) %> 
        </tbody>
      </table>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
